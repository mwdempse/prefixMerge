# Example using Chick Weight data set
# Assumes that prefixMerge is loaded
# Run code below if not loaded
# source('https://raw.githubusercontent.com/mwdempse/prefixMerge-/main/prefixMerge.R')

# dummy data
df1 <- head(ChickWeight,10)
df2 <- tail(ChickWeight,10)

# base merge
merge(df1,df2, by="Time")

# custom merge
prefixMerge(df1,df2, by="Time")
  
  
  
